{
  "name": "dotenv-webpack",
  "description": "A simple webpack plugin to support dotenv.",
  "version": "0.0.0-semantically-released",
  "main": "index.js",
  "scripts": {
    "precommit": "npm run lint",
    "prepush": "npm t",
    "lint": "standard",
    "predoc": "rimraf docs",
    "doc": "esdoc -c config/doc.json",
    "pretest": "npm run build",
    "test": "nyc _mocha --compilers js:babel-register",
    "posttest": "nyc report --reporter=lcov",
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist src",
    "travis": "npm run lint && npm t"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mrsteele/dotenv-webpack.git"
  },
  "keywords": [
    "dotenv",
    "env",
    "safe",
    "environment",
    "dotenv-safe",
    "variables",
    "process",
    "process.env",
    "webpack",
    "plugin"
  ],
  "author": "Matt Steele <matt@omnionline.us> (http://omnionline.us/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrsteele/dotenv-webpack/issues"
  },
  "homepage": "https://github.com/mrsteele/dotenv-webpack#readme",
  "peerDependencies": {
    "webpack": "^1 || ^2 || ^3 || ^4"
  },
  "dependencies": {
    "dotenv": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "8.0.3",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "esdoc": "^1.0.4",
    "husky": "^0.14.3",
    "mocha": "^4.0.0",
    "nyc": "^11.3.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.1.3",
    "standard": "^10.0.3",
    "webpack": "^3.10.0"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "standard": {
    "parser": "babel-eslint"
  },
  "browser": "browser.js",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-assign"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.js",
      "dist/**/*.js"
    ]
  }
}
